{
  "components/common/navigation": "<nav class=\"navUser\" role=\"navigation\" aria-label=\"User Navigation\">\n    {{> components/common/currency-selector}}\n\n    <ul class=\"navUser-section navUser-section--alt\">\n        {{#if customer.store_credit.value '>' 0}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action navUser-action--storeCredit\"\n                   data-dropdown=\"storeCredit-dropdown\"\n                   data-options=\"align:bottom\"\n                   href=\"{{urls.cart}}\"\n                   aria-label=\"{{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\"\n                >\n                    <span class=\"navUser-action-divider\">\n                        {{lang 'common.store_credit_overview' credit=customer.store_credit.formatted}}\n                    </span>\n                </a>\n                <div class=\"dropdown-menu\" id=\"storeCredit-dropdown\" data-dropdown-content aria-hidden=\"true\">\n                    {{{lang 'common.store_credit' store_credit=customer.store_credit.formatted}}}\n                </div>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-item--compare\"\n               href=\"{{urls.compare}}\"\n               data-compare-nav\n               aria-label=\"{{lang 'common.compare'}}\"\n            >\n                {{lang 'common.compare'}} <span class=\"countPill countPill--positive countPill--alt\"></span>\n            </a>\n        </li>\n        {{#if theme_settings.social_icon_placement_top}}\n            <li class=\"navUser-item navUser-item--social\">\n                {{> components/common/social-links}}\n            </li>\n            <li class=\"navUser-item navUser-item--divider\">|</li>\n        {{/if}}\n        <li class=\"navUser-item\">\n            <a class=\"navUser-action navUser-action--quickSearch\"\n               href=\"#\" data-search=\"quickSearch\"\n               aria-controls=\"quickSearch\"\n               aria-expanded=\"false\"\n               aria-label=\"{{lang 'common.search'}}\"\n            >\n                {{lang 'common.search'}}\n            </a>\n        </li>\n        {{#if settings.gift_certificates_enabled}}\n            <li class=\"navUser-item\">\n                <a class=\"navUser-action\"\n                   href=\"{{urls.gift_certificate.purchase}}\"\n                   aria-label=\"{{lang 'common.gift_cert'}}\"\n                >\n                    {{lang 'common.gift_cert'}}\n                </a>\n            </li>\n        {{/if}}\n        <li class=\"navUser-item navUser-item--account\">\n            {{#if customer}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.account.index}}\"\n                   aria-label=\"{{lang 'common.account'}}\"\n                >\n                    {{lang 'common.account'}}\n                </a>\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.logout}}\"\n                   aria-label=\"{{lang 'common.logout'}}\"\n                >\n                    {{lang 'common.logout'}}\n                </a>\n            {{else}}\n                <a class=\"navUser-action\"\n                   href=\"{{urls.auth.login}}\"\n                   aria-label=\"{{lang 'common.login'}}\"\n                >\n                    {{lang 'common.login'}}\n                </a>\n                {{#if settings.account_creation_enabled}}\n                    <span class=\"navUser-or\">{{lang 'common.or'}}</span>\n                    <a class=\"navUser-action\"\n                       href=\"{{urls.auth.create_account}}\"\n                       aria-label=\"{{lang 'common.sign_up'}}\"\n                    >\n                        {{lang 'common.sign_up'}}\n                    </a>\n                {{/if}}\n            {{/if}}\n        </li>\n        <li class=\"navUser-item navUser-item--cart\">\n            <a class=\"navUser-action\"\n               data-cart-preview\n               data-dropdown=\"cart-preview-dropdown\"\n               data-options=\"align:right\"\n               href=\"{{urls.cart}}\"\n               aria-label=\"{{lang 'cart.nav_aria_label'}}\"\n            >\n                <span class=\"navUser-item-cartLabel\">{{lang 'common.cart'}}</span>\n                <span class=\"countPill cart-quantity\"></span>\n            </a>\n\n            <div class=\"dropdown-menu\" id=\"cart-preview-dropdown\" data-dropdown-content aria-hidden=\"true\"></div>\n        </li>\n    </ul>\n    <div class=\"dropdown dropdown--quickSearch\" id=\"quickSearch\" aria-hidden=\"true\" tabindex=\"-1\" data-prevent-quick-search-close>\n        {{> components/common/quick-search}}\n    </div>\n</nav>\n",
  "components/common/currency-selector": "<a href=\"#\" class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\n    <span class=\"flag\">{{{currency_selector.active_currency_flag}}}</span> \n    <span class=\"text\">\n        {{currency_selector.active_currency_code}}\n    </span>\n    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n    </span>\n</a>\n<div class=\"navPage-subMenu navPage-subMenu-horizontal\">\n    <ul class=\"navPage-subMenu-list\">\n        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n            <p class=\"navPage-subMenu-action navPages-action\">\n                <span class=\"text\">\n                    Currency\n                </span>\n            </p>\n        </li>\n        {{#each currency_selector.currencies}}\n            <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                <a class=\"navPage-subMenu-action navPages-action{{#if is_active}} navPages-action-active{{/if}}\" href=\"{{{switch_url}}}\">\n                    <span class=\"flag\">{{{flag}}}</span>\n                    <span class=\"text\">{{code}}</span>\n                </a>\n            </li>\n        {{/each}}\n    </ul>\n</div>\n",
  "components/common/social-links": "<ul class=\"socialLinks socialLinks--alt\">\n\t{{#if position '==' 'footer'}}\n        {{#each social_media}}\n            <li class=\"socialLinks-item socialLinks-item--{{display_name}}\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-messenger\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n\t{{#if position '==' 'newsletter'}}\n\t\t{{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}2\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}2\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger2\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-messenger2\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat2\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat2\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n</ul>\n\n",
  "components/common/quick-search": "<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            {{#if theme_settings.halo-search-category}}\n                <div id=\"haloSearchCategory\">\n                    <label class=\"is-srOnly\" for=\"search_category_query\">{{lang 'header.categories'}}</label>\n                    <select name=\"category\" class=\"halo-select-category\" id=\"search_category_query\">\n                        <option value=\"\" selected=\"\">{{lang 'header.categories'}}</option>\n                        {{#unless page_type '==' 'default'}}\n                            {{#each categories}}\n                                <option value=\"{{id}}\">{{name}}</option>\n                                {{#if children}}\n                                    {{#each children}}\n                                        <option value=\"{{id}}\"><span>---</span> {{name}}</option>\n                                        {{#if children}}\n                                            {{#each children}}\n                                                <option value=\"{{id}}\"><span>------</span> {{name}}</option>\n                                                {{#if children}}\n                                                    {{#each children}}\n                                                        {{#if children}}\n                                                            <option value=\"{{id}}\"><span>---------</span> {{name}}</option>\n                                                        {{else}}\n                                                            <option value=\"{{id}}\"><span>---------</span> {{name}}</option>\n                                                        {{/if}}\n                                                    {{/each}}\n                                                {{/if}}\n                                            {{/each}}\n                                        {{/if}}\n                                    {{/each}}\n                                {{/if}}\n                            {{/each}}\n                        {{/unless}}\n                    </select> \n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                </div>\n            {{/if}}\n            <label class=\"is-srOnly\" for=\"search_query\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" data-search-quick name=\"search_query\" id=\"search_query\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            <button type=\"submit\" class=\"button button--primary\" aria-label=\"{{lang 'search.quick_search.button_search'}}\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg></button>\n        </div>\n    </fieldset>\n</form>\n<div class=\"haloQuickSearchResults quickSearchResults\" data-bind=\"html: results\"></div>\n{{#if theme_settings.halo-before-quick-results}}\n    <div class=\"haloQuickSearchResults quickSearchResultsCustom\">\n        {{> components/halothemes/halo-before-quick-results productsnew=products.new}}\n    </div>\n{{/if}}\n{{~inject 'quickSearchBefore' theme_settings.halo-before-quick-results}}\n",
  "components/halothemes/halo-before-quick-results": "<div class=\"quickResults-wrapper\">\n    {{#if theme_settings.halo-before-quick-results-trending-now}}\n        <div class=\"quickResults-item quickResults-text\">\n            {{#if theme_settings.halo-before-quick-results-trending-now-title}}\n                <h3 class=\"quickResults-title\">{{theme_settings.halo-before-quick-results-trending-now-title}}</h3>\n            {{/if}}\n            <ul class=\"productGrid-search\">\n                {{#if theme_settings.halo-before-quick-results-trending-now-1}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-1)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-1}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-2}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-2)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-2}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-3}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-3)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-3}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-4}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-4)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-4}}\n                        </a>\n                    </li>\n                {{/if}}\n                {{#if theme_settings.halo-before-quick-results-trending-now-5}}\n                    <li class=\"search-item\">\n                        <a class=\"link\" href=\"/search.php?search_query={{lowercase (plusify theme_settings.halo-before-quick-results-trending-now-5)}}&section=product\">\n                            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search\"></use></svg>{{theme_settings.halo-before-quick-results-trending-now-5}}\n                        </a>\n                    </li>\n                {{/if}}\n            </ul>\n        </div>\n    {{/if}}\n    {{#if theme_settings.halo-before-quick-results-popular}}\n        <div class=\"quickResults-item quickResults-product\">\n            {{#if theme_settings.halo-before-quick-results-popular-title}}\n                <h3 class=\"quickResults-title\">{{theme_settings.halo-before-quick-results-popular-title}}</h3>\n            {{/if}}\n            <div class=\"loadingOverlay\"></div>\n            <ul class=\"productGrid\"></ul>\n        </div>\n    {{/if}}\n</div>\n{{~inject 'quickSearchPopularId' theme_settings.halo-before-quick-results-popular-id}}\n{{~inject 'quickSearchResultLimit' theme_settings.halo-before-quick-results-limit}}\n"
}