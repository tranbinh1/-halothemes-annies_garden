{
  "components/halothemes/halo-ask-an-expert-content": "<div class=\"modal-header\">\n    <h2 class=\"modal-header-title\">{{theme_settings.halo-ask-an-expert-title}}</h2>\n</div>\n{{#if theme_settings.halo-ask-an-expert-logo}}\n<div class=\"halo-ask-an-expert-logo\">\n    <a href=\"/\">\n        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo-ask-an-expert-logo}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n    </a>\n</div>\n{{/if}}\n\n<div class=\"modal-body\" {{#if page_type '==' 'product'}}data-product-id=\"{{product.id}}\" data-product-title=\"{{product.title}}\" data-product-url=\"{{product.url}}\" data-product-sku=\"{{product.sku}}\" data-product-image=\"{{getImage product.main_image}}\"{{/if}}>\n    <div class=\"halo-ask-an-expert-subtitle\">\n        <span>{{theme_settings.halo-ask-an-expert-subtitle}}</span>\n    </div>\n    <div class=\"halo-ask-an-expert-text\">{{theme_settings.halo-ask-an-expert-text1}}<br>{{theme_settings.halo-ask-an-expert-text2}}</div>\n    <div id=\"halo-ask-an-expert-results\" class=\"halo-ask-an-expert-text\"></div>\n    <form class=\"form\" id=\"halo-ask-an-expert-form\">\n        <fieldset class=\"form-fieldset\">\n            <div class=\"form-field\">\n            \t<label class=\"form-label\" for=\"customer_name\">Your name</label>\n                <input required type=\"text\" class=\"form-input\" name=\"customer_name\" id=\"customer_name\">\n            </div>\n            <div class=\"form-field\">\n            \t<label class=\"form-label\" for=\"customer_email\">Email address</label>\n                <input required type=\"text\" class=\"form-input\" name=\"customer_email\" id=\"customer_email\">\n            </div>\n            <div class=\"form-field\">\n            \t<label class=\"form-label\" for=\"customer_phone\">Phone number</label>\n                <input required type=\"tel\" class=\"form-input\" name=\"customer_phone\" id=\"customer_phone\">\n            </div>\n            <div class=\"form-field form-field-radio\">\n                <label class=\"form-label\">Do you need:</label>\n                <input class=\"form-radio\" id=\"pricing-radio\" type=\"radio\" name=\"type_package\" checked value=\"Pricing\">\n                <label class=\"form-label\" for=\"pricing-radio\">Pricing</label>\n                <input class=\"form-radio\" id=\"answers-radio\" type=\"radio\" name=\"type_package\" value=\"Answers\">\n                <label class=\"form-label\" for=\"answers-radio\">Answers</label>\n                <input class=\"form-radio\" id=\"both-radio\" type=\"radio\" name=\"type_package\" value=\"Pricing & Answers\">\n                <label class=\"form-label\" for=\"both-radio\">Both</label>\n            </div>\n            <div class=\"form-field\">\n                <label class=\"form-label\" for=\"comment-area\">What can i help you with today?</label>\n                <textarea name=\"message\" cols=\"30\" rows=\"6\" class=\"form-input\" id=\"comment-area\" required></textarea>\n            </div>\n            <div class=\"form-field form-field-radio form-field-radio2\">\n                <label class=\"form-label form-label-custom\">How would you like me to contact you?</label>\n                <input class=\"form-radio\" id=\"phone-radio\" type=\"radio\" name=\"type_contact\" checked value=\"Phone\">\n                <label class=\"form-label\" for=\"phone-radio\">Phone</label>\n                <input class=\"form-radio\" id=\"email-radio\" type=\"radio\" name=\"type_contact\" value=\"Email\">\n                <label class=\"form-label\" for=\"email-radio\">Email</label>\n                <input class=\"form-radio\" id=\"both-radio-2\" type=\"radio\" name=\"type_contact\" value=\"Phone & Email\">\n                <label class=\"form-label\" for=\"both-radio-2\">Both</label>\n            </div>\n            <div class=\"form-field form-field-checkbox\">\n                <input type=\"checkbox\" id=\"term-ask\" name=\"new\" value=\"Terms Conditions\">\n                <label class=\"form-label\" for=\"term-ask\">\n                    I agree with the <span>Data Protection Policy</span> for the Annie's Garden Ask an Expert Form\n                </label>\n            </div>\n            <div class=\"form-actions\">\n               <button type=\"button\" class=\"button button--primary\" id=\"halo-ask-an-expert-button\" disabled>Submit</button>\n            </div>\n        </fieldset>\n    </form>\n</div>\n"
}