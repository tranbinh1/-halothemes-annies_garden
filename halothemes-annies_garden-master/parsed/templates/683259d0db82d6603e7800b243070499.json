{
  "components/cart/modals/configure-product-2": "<div class=\"product-edit-detail\">\n    <div class=\"product-edit\" data-product-edit-id=\"{{product.id}}\" data-product-edit-item-id>\n        <div class=\"product-edit-image\"></div>\n        <div class=\"product-edit-content\">\n            <div class=\"product-edit-brand\">{{product.brand.name}}</div>\n            <h4 class=\"product-edit-title\">{{product.title}}</h4>\n            <div class=\"product-edit-variant\"></div>\n        </div>\n    </div>\n</div>\n\n<div class=\"alertMessageBox\" hidden>\n    {{> components/common/alert-info}}\n</div>\n\n<div class=\"product-edit-info\">\n    <div class=\"product-edit-change\" data-product-option-change2>\n        <form class=\"form\" method=\"post\" action=\"cart.php\" enctype=\"multipart/form-data\">\n            <input type=\"hidden\" name=\"action\" value=\"add\">\n            <input type=\"hidden\" name=\"product_id\" value=\"{{product.id}}\"/>\n            <div class=\"product-edit-option\">\n                <table>\n                    <thead class=\"cart-edit-row cart-edit-row--heading\">\n                        <tr>\n                            {{#each product.options}}\n                                <th class=\"text-left\">{{display_name}}</th>\n                            {{/each}}\n                            <th class=\"text-left\">{{lang 'cart.checkout.quantity'}}</th>\n                            <th class=\"text-right\">{{lang 'cart.checkout.unit_price'}}</th>\n                            <th class=\"text-right\"></th>\n                        </tr>\n                    </thead>\n                    <tbody>\n                        <tr class=\"cart-edit-row\" data-cart-edit-item>\n                            <td class=\"cart-edit-quantity text-left\" data-cart-edit-item-quantity>\n                                <div class=\"form-increment\" data-quantity-change>\n                                    <input class=\"form-input form-input--incrementTotal\"\n                                        id=\"qty-{{id}}\"\n                                        name=\"qty[]\"\n                                        type=\"number\"\n                                        value=\"{{#if product.min_purchase_quantity}}{{product.min_purchase_quantity}}{{else}}1{{/if}}\"\n                                        data-quantity-min=\"{{product.min_purchase_quantity}}\"\n                                        data-quantity-max=\"{{product.max_purchase_quantity}}\"\n                                        data-preval=\"\"\n                                        min=\"1\"\n                                        data-cart-itemid=\"{{id}}\"\n                                        pattern=\"[0-9]*\"\n                                        aria-live=\"polite\">  \n                                </div>\n                            </td>\n                            <td class=\"cart-edit-price text-right\" data-cart-edit-item-price>\n                                {{#if product.price.with_tax}}\n                                    <span class=\"price\">{{product.price.with_tax.formatted}}</span>\n                                {{/if}}\n                                {{#if product.price.without_tax}}\n                                    <span class=\"price\">{{product.price.without_tax.formatted}}</span>\n                                {{/if}}\n                            </td>\n                            <td class=\"cart-edit-remove text-right\" data-cart-edit-item-remove>\n                                <a href=\"#\">Ã—</a>\n                            </td>\n                        </tr>\n                    </tbody>\n                </table>\n            </div>\n        </form>\n    </div>\n</div>\n\n<div class=\"product-edit-action\">\n    <div class=\"product-add-more\">\n        <a class=\"product-addmore-button\" href=\"#\">{{lang 'cart.checkout.add_more'}}</a>\n    </div>\n    <div class=\"product-edit-button\">\n        <button class=\"button button--primary\" data-edit-cart-add-to-cart>{{lang 'products.add_to_cart'}}</button>\n    </div>\n</div>\n",
  "components/common/alert-info": "<div class=\"alertBox alertBox--info {{{sanitize this}}}\">\n    <div class=\"alertBox-column alertBox-icon\">\n        <icon glyph=\"ic-success\" class=\"icon\" aria-hidden=\"true\"><svg xmlns=\"http://www.w3.org/2000/svg\" width=\"24\" height=\"24\" viewBox=\"0 0 24 24\"><path d=\"M12 2C6.48 2 2 6.48 2 12s4.48 10 10 10 10-4.48 10-10S17.52 2 12 2zm1 15h-2v-2h2v2zm0-4h-2V7h2v6z\"></path></svg></icon>\n    </div>\n    <p class=\"alertBox-column alertBox-message\">\n        <span>{{{this}}}</span>\n    </p>\n</div>\n"
}