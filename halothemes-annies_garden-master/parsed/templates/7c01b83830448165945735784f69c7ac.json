{
  "components/products/options/set-radio": "<div class=\"form-field\" data-product-attribute=\"set-radio\" data-product-option-id=\"{{id}}\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\">\n        {{ this.display_name }}\n\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    {{#unless required}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_radio_{{../id}}_none\"\n            name=\"attribute[{{../id}}]\"\n            value=\"\"\n            checked=\"{{#if defaultValue '==' ''}}checked{{/if}}\"\n            data-title=\"{{this.label}}\"\n            data-index=\"{{@index}}\"\n        >\n        <label class=\"form-label\" for=\"attribute_radio_{{../id}}_none\">{{lang 'products.none'}}</label>\n    {{/unless}}\n\n    {{#each this.values}}\n        <input\n            class=\"form-radio\"\n            type=\"radio\"\n            id=\"attribute_radio_{{../id}}_{{id}}\"\n            name=\"attribute[{{../id}}]\"\n            value=\"{{id}}\"\n            {{#if selected}}\n                checked\n                data-default\n            {{/if}}\n            {{#if ../required}}required{{/if}}\n            data-title=\"{{this.label}}\"\n            data-index=\"{{@index}}\"\n        >\n        <label data-product-attribute-value=\"{{id}}\" class=\"form-label\" for=\"attribute_radio_{{../id}}_{{id}}\">{{this.label}}</label>\n    {{/each}}\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}{{lang 'common.required'}}{{else}}{{lang 'common.optional'}}{{/if}}\n</small>\n"
}