{
  "components/halothemes/products/product-view-tabs": "{{#if theme_settings.halo-productView-tabs}}\n\t<div class=\"productView-tabs halo-block\" id=\"halo-productView-tabs\" data-tabs-background=\"{{#if theme_settings.halo-productView-tabs-background}}https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo-productView-tabs-background}}{{/if}}\">\n\t\t<div{{#if theme_settings.halo-productView-tabs-background}} class=\"productView-tabs-image\" style=\"background-image: url(https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo-productView-tabs-background}})\"{{else}} class=\"productView-tabs-image noImage\"{{/if}}>&nbsp;</div>\n\t\t<div class=\"productView-tab\">\n\t\t\t{{#if theme_settings.halo-productView-tabs-title}}\n\t\t\t\t<div class=\"productView-tab-header\">\n\t\t\t\t\t<h2 class=\"title\">{{theme_settings.halo-productView-tabs-title}}</h2>\n\t\t\t\t</div>\n\t\t\t{{/if}}\n\t\t\t<ul class=\"tabs\" data-tab>\n\t\t\t\t{{#if theme_settings.halo-productView-tabs-1-title}}\n\t\t\t\t    <li class=\"tab is-active\">\n\t\t\t\t        <a class=\"tab-title\" href=\"#tab-1\" data-tab-id=\"tab-1\">{{theme_settings.halo-productView-tabs-1-title}}</a>\n\t\t\t\t    </li>\n\t\t\t    {{/if}}\n\t\t\t    {{#if theme_settings.halo-productView-tabs-2-title}}\n\t\t\t        <li class=\"tab productView-tab-2{{#unless theme_settings.halo-productView-tabs-1-title}} is-active{{/unless}}\">\n\t\t\t            <a class=\"tab-title\" href=\"#tab-2\" data-tab-id=\"tab-2\">{{theme_settings.halo-productView-tabs-2-title}}</a>\n\t\t\t        </li>\n\t\t\t    {{/if}}\n\t\t\t</ul>\n\t\t\t<div class=\"tabs-contents\">\n\t\t\t\t<div class=\"loadingOverlay\"></div>\n\t\t\t\t{{#if theme_settings.halo-productView-tabs-1-title}}\n\t\t\t\t    <div class=\"tab-content is-active\" id=\"tab-1\">\n\t\t\t\t      \t<div class=\"toggle-title\">\n\t\t\t\t          \t<a class=\"toggleLink is-open\" data-c