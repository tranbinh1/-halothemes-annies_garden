{
  "components/halothemes/halo-newsletter-popup": "{{#if theme_settings.halo_newsletter_popup}}\n    <div id=\"halo-newsletter-popup\" class=\"hide\">\n        <div class=\"newsletter-popup-container{{#if theme_settings.halo_newsletter_popup_image}} hasImage {{else}} noImage{{/if}}\">\n            <div class=\"newsletter-popup-inner\">\n                <button type=\"button\" data-close-newsletter-popup aria-label=\"Close Newsletter Popup\">\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\n                </button>\n                {{#if theme_settings.halo_newsletter_popup_image}}\n                    <div class=\"newsletter-left\">\n                        <img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo_newsletter_popup_image}}\" alt=\"{{theme_settings.halo_newsletter_popup_heading}}\" title=\"{{theme_settings.halo_newsletter_popup_heading}}\">\n                    </div>\n                {{/if}}\n                <div class=\"newsletter-right{{#if theme_settings.halo_newsletter_popup_image}} hasImage {{else}} noImage{{/if}}\">\n                    <h2 class=\"newsletter-heading\">{{theme_settings.halo_newsletter_popup_heading}}</h2>\n                    {{#or (if theme_settings.halo_newsletter_popup_summary1) (if theme_settings.halo_newsletter_popup_summary2)}}\n                        <div class=\"newsletter-message\">\n                            <span>{{theme_settings.halo_newsletter_popup_summary1}}</span>\n                            <span>{{theme_settings.halo_newsletter_popup_summary2}}</span>\n                        </div>\n                    {{/or}}\n                    <div id=\"popupSubcribeForm\">\n                        <form class=\"form\" action=\"{{urls.subscribe.action}}\" method=\"post\" id=\"popupSubcribeFormSubmit\">\n                            <fieldset class=\"form-fieldset\">\n                                <input type=\"hidden\" name=\"action\" value=\"subscribe\">\n                                <input type=\"hidden\" name=\"nl_first_name\" value=\"bc\">\n                                <input type=\"hidden\" name=\"check\" value=\"1\">\n                                <div class=\"form-field\">\n                                    <label class=\"form-label is-srOnly\" for=\"nl_email2\">{{lang 'forms.contact_us.email_2'}}:</label>\n                                    <input class=\"form-input\" id=\"nl_email2\" name=\"nl_email\" type=\"email\" placeholder=\"{{lang 'forms.contact_us.email'}}\">\n                                    <input class=\"button button--primary button--large\" type=\"submit\" value=\"{{lang 'forms.contact_us.submit_2'}}\">\n                                </div>\n                            </fieldset>\n                        </form>\n                    </div>\n                    {{#if theme_settings.halo_newsletter_popup_again}}\n                        <div class=\"form-show-again\">\n                            <form>\n                                <input id=\"do-not-show-again\" class=\"do-not-show-again\" type=\"checkbox\" name=\"don't show again\" value=\"1\"> \n                                <label for=\"do-not-show-again\">{{theme_settings.halo_newsletter_popup_again}}</label>\n                            </form>\n                        </div>\n                    {{/if}}\n                    {{#all (if theme_settings.show_social-links) (if theme_settings.halo_newsletter_popup_social) (if social_media)}}\n                        <div class=\"newsletter-socials\">\n                            {{> components/common/social-links position=\"newsletter\"}}\n                        </div>\n                    {{/all}}\n                </div>\n            </div>\n        </div>\n    </div>\n{{/if}}\n",
  "components/common/social-links": "<ul class=\"socialLinks socialLinks--alt\">\n\t{{#if position '==' 'footer'}}\n        {{#each social_media}}\n            <li class=\"socialLinks-item socialLinks-item--{{display_name}}\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-messenger\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n\t{{#if position '==' 'newsletter'}}\n\t\t{{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}2\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}2\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger2\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-messenger2\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat2\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat2\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n</ul>\n\n"
}