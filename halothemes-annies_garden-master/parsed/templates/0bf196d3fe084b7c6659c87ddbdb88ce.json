{
  "components/common/wishlist-dropdown": "<form action=\"{{product.add_to_wishlist_url}}\" class=\"form form-wishlist{{#unless product.out_of_stock}}{{#if theme_settings.halo_buy_it_now}} form-wishlist-custom{{/if}}{{/unless}}\" data-wishlist-add method=\"post\">\n    <a aria-controls=\"wishlist-dropdown-{{product.id}}\" aria-expanded=\"false\" class=\"dropdown-menu-button\" data-dropdown=\"wishlist-dropdown-{{product.id}}\" href=\"#\" aria-label=\"Add to Wishlist\">\n        <span class=\"text\">Add to Wishlist</span>\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-heart\"></use></svg>\n    </a>\n    <ul aria-hidden=\"true\" class=\"dropdown-menu\" data-dropdown-content id=\"wishlist-dropdown-{{product.id}}\" tabindex=\"-1\">\n        {{#if customer.wishlists}} \n            {{#each customer.wishlists}}\n            <li>\n                <input class=\"button {{#if this.num_items '!==' '0'}}button--has-items{{/if}}\" formaction=\"{{this.add_url}}&product_id={{../product.id}}\"\n                    type=\"submit\" value=\"{{this.name}}\">\n            </li>\n            {{/each}} \n        {{else}}\n            <li>\n                <input class=\"button\" type=\"submit\" value=\"{{lang 'account.wishlists.add_to_default'}}\">\n            </li>\n        {{/if}}\n        <li>\n            <a data-wishlist class=\"button\" href=\"/wishlist.php?action=addwishlist&product_id={{product.id}}\">{{lang 'account.wishlists.create_new'}}</a>\n        </li>\n    </ul>\n</form>\n"
}