{
  "components/search/content-listing": "{{#if content_results}}\n    {{> components/search/content-sort-box sort=pagination.content_results.sort}}\n    <div>\n        {{#each content_results}}\n            <strong><a href=\"{{url}}\">{{{title}}}</a></strong> ({{type}})\n            <p>{{{content}}}</p>\n        {{/each}}\n    </div>\n    {{> components/common/paginator pagination.content_results reload=true}}\n{{else}}\n\t<p class=\"page-no-result\">{{lang 'search_results.no_results'}} \"{{sanitize forms.search.query}}\"</p>\n{{/if}}\n",
  "components/search/content-sort-box": "<form class=\"actionBar\" method=\"get\" data-sort-by>\n    <fieldset class=\"form-fieldset actionBar-section\">\n        <div class=\"form-field\">\n            <label class=\"form-label\" for=\"sort\">{{lang 'common.sorter.sort_by'}}</label>\n            <select class=\"form-select\" name=\"sort\" id=\"sort\">\n                <option value=\"relevance\" {{#if sort '==' 'relevance'}}selected{{/if}}>{{lang 'common.sorter.relevance'}}</option>\n                <option value=\"alphaasc\" {{#if sort '==' 'alphaasc'}}selected{{/if}}>{{lang 'common.sorter.alpha_asc'}}</option>\n                <option value=\"alphadesc\" {{#if sort '==' 'alphadesc'}}selected{{/if}}>{{lang 'common.sorter.alpha_desc'}}</option>\n            </select>\n        </div>\n    </fieldset>\n</form>\n",
  "components/common/paginator": "<nav class=\"pagination\" aria-label=\"pagination\">\n  {{#and (if total) (if page_type '==' 'category')}}\n      {{#if start}}\n          {{#if end}}\n              {{#if end '<' total}}\n                  <div class=\"pagination-info\">Items <span class=\"start\">{{start}}</span> to <span class=\"end\">{{end}}</span> of <span class=\"total\">{{total}}</span> total</div>\n              {{else}}\n                  <div class=\"pagination-info\">Items <span class=\"start\">{{start}}</span> to <span class=\"end\">{{total}}</span> of <span class=\"total\">{{total}}</span> total</div>\n              {{/if}}\n          {{/if}}\n      {{/if}}\n  {{/and}}\n  {{#and (if page_type '==' 'brand') (if links.length)}}\n      {{#each shop_by_brand}}\n          {{#if name '==' ../brand.name}}\n              <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{count}}</span> total</div>\n          {{/if}}\n      {{/each}}\n  {{/and}}\n  {{#if page_type '==' 'search'}}\n      <div class=\"pagination-info\">Items <span class=\"start\"></span> to <span class=\"end\"></span> of <span class=\"total\">{{total}}</span> total</div>\n  {{/if}}\n    <ul class=\"pagination-list\">\n        {{#if previous}}\n            <li class=\"pagination-item pagination-item--previous\">\n                <a class=\"pagination-link\"\n                   href=\"{{previous}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.previous'}}\"\n                >\n                    <svg class=\"icon\" aria-hidden=\"true\">\n                        <use xlink:href=\"#icon-chevron-left\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n\n        {{#each links}}\n            <li class=\"pagination-item {{#if this.number '==' ../current}}pagination-item--current{{/if}}\">\n                <a class=\"pagination-link\"\n                   href=\"{{this.url}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   {{#if this.number '==' ../current}}aria-current=\"page\"{{/if}}\n                   aria-label=\"{{lang 'common.paginator.page_of' current=this.number total=../links.length}}\"\n                >\n                    {{this.number}}\n                </a>\n            </li>\n        {{/each}}\n\n        {{#if next}}\n            <li class=\"pagination-item pagination-item--next\">\n                <a class=\"pagination-link\"\n                   href=\"{{next}}\"\n                   {{#unless reload}}data-faceted-search-facet{{/unless}}\n                   aria-label=\"{{lang 'common.next'}}\"\n                >\n                    <svg class=\"icon\" aria-hidden=\"true\">\n                        <use xlink:href=\"#icon-chevron-right\"></use>\n                    </svg>\n                </a>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n"
}