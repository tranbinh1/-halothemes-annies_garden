{
  "components/halothemes/halo-recently-viewed-products": "{{#if theme_settings.halo_recently_viewed_products }}\n    <section class=\"recently-viewed-products-sidebar\"{{#unless settings.data_tag_enabled}} data-tag-enabled=\"false\"{{/unless}}>\n        <div class=\"lst-seen-widget\">\n            <div class=\"collapse-icon\">\n                <svg class=\"icon\" aria-label=\"Collapse Recently Viewed Product popup\"><use xlink:href=\"#icon-collapse\"></use></svg>\n            </div>\n            <div class=\"top\">\n                <div class=\"cart-icon with-products\">\n                    <a href=\"#\" data-cart-preview2 aria-label=\"{{lang 'cart.nav_aria_label'}}\">\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cart\"></use></svg>\n                        <span class=\"cart-quantity {{#if cart.items}} countPill--positive{{/if}}\">{{cart.quantity}}</span>\n                    </a>\n                </div>\n            </div>\n            <div class=\"in-content\">\n                {{#if theme_settings.recently_viewed_products_title}}\n                    <h3 class=\"title\">{{theme_settings.recently_viewed_products_title}}</h3>\n                {{/if}}\n                <div class=\"products-grid\" id=\"recently-viewed-products-list\">\n                    <div class=\"no-products\" style=\"flex-direction: column;display:none;\">\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-no-products\"></use></svg>\n                        <div class=\"text\">Sorry, there are no products.</div>\n                    </div>\n                </div>\n                <div id=\"recently-viewed-products-list-tmp\" style=\"display: none !important;\"></div>\n            </div>\n            {{#if theme_settings.halo_button_top }}\n                <div class=\"bottom backtoTop\">\n                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-cross-line-top2\"></use></svg>\n                    <div class=\"text\">{{theme_settings.halo_button_top}}</div>\n                </div>\n            {{/if}}\n        </div>\n        <div class=\"wrap-icons\">\n            <div class=\"expand\">\n                <svg class=\"icon\" aria-label=\"Expand Recently Viewed Product popup\"><use xlink:href=\"#icon-expand\"></use></svg>\n            </div>\n            <div class=\"backtoTop\">\n                <svg class=\"icon\" aria-label=\"{{theme_settings.halo_button_top}}\"><use xlink:href=\"#icon-cross-line-top1\"></use></svg>\n            </div>\n        </div>\n    </section>\n{{/if}}\n"
}