{
  "components/halothemes/sidebar/halo-sidebar": "<div class=\"halo-sidebar halo-menu-mobile halo-sidebar-left{{#if theme_settings.haloMegamenu}} halo-menu-megamenu{{/if}}\" id=\"halo-menu-mobile\">\n    <a href=\"#\" class=\"halo-sidebar-close\" aria-label=\"Close Menu Sidebar\">\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\n    </a>\n    <div class=\"halo-sidebar-wrapper navPages-container\" data-menu>\n        {{> components/common/navigation-mobile }}\n    </div>\n</div>\n\n<div class=\"halo-sidebar halo-search-mobile halo-sidebar-left\" id=\"halo-search-mobile\">\n    <a href=\"#\" class=\"halo-sidebar-close\" aria-label=\"Close Search Sidebar\">\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\n    </a>\n    <div class=\"halo-sidebar-wrapper\"></div>\n</div>\n\n{{#if page_type '!=' 'login'}}\n    <div class=\"halo-sidebar halo-auth-sidebar halo-sidebar-right\" id=\"{{#if customer}}halo-account-mobile{{else}}halo-login-mobile{{/if}}\">\n        <a href=\"#\" class=\"halo-sidebar-close\" aria-label=\"Close Account Sidebar\">\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\n        </a>\n        <div class=\"halo-sidebar-wrapper\">\n            {{#if customer}}\n                {{> components/halothemes/halo-account-popup data=\"account-mb\"}}\n            {{else}}\n                {{> components/halothemes/halo-login-popup data=\"login-mb\" email_id=\"login_email2\" password_id=\"login_pass2\"}}\n            {{/if}}\n        </div>\n    </div>\n{{/if}}\n\n<div class=\"halo-sidebar halo-cart-sidebar halo-sidebar-right\" id=\"halo-cart-sidebar\">\n    <a href=\"#\" class=\"halo-sidebar-close\" aria-label=\"Close Cart Sidebar\">\n        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\n    </a>\n    <div class=\"halo-sidebar-wrapper\"></div>\n</div>\n\n{{#if theme_settings.halobeforeyouleave}}\n    <div class=\"halo-sidebar halo-leave-sidebar halo-sidebar-right\" id=\"halo-before-you-leave\" data-time=\"{{multiply theme_settings.halo-before-you-leave-time 60000}}\">\n        <a href=\"#\" class=\"halo-sidebar-close\" aria-label=\"Close Before You Leave Popup\">\n            <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-close\"></use></svg>\n        </a>\n        <div class=\"halo-sidebar-wrapper\">\n            {{> components/halothemes/halo-before-you-leave}}\n        </div>\n    </div>\n{{/if}}\n",
  "components/common/navigation-mobile": "<nav class=\"navPages\" role=\"navigation\" aria-label=\"Main Navigation\">\n    <ul class=\"navPages-list\"></ul>\n    <ul class=\"navPages-list navPages-list--user\">\n        <li class=\"navPages-item navPages-item-user\">\n            {{#if customer}}\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.account.index}}\" aria-label=\"{{lang 'account.title'}}\">\n                    <span class=\"text\">\n                        <svg class=\"icon\" aria-hidden=\"true\">\n                            <use xlink:href=\"#icon-user\"></use>\n                        </svg>\n                        {{lang 'account.title'}}\n                    </span>\n                </a>\n            {{else}}\n                <a class=\"navPages-action navPages-action-end\" href=\"{{urls.auth.login}}\" aria-label=\"{{lang 'login.title'}}\">\n                    <span class=\"text\">\n                        <svg class=\"icon\" aria-hidden=\"true\">\n                            <use xlink:href=\"#icon-user\"></use>\n                        </svg>\n                        {{lang 'login.title'}}\n                    </span>\n                </a>\n            {{/if}}\n        </li>\n        <li class=\"navPages-item navPages-item-user\">\n            <a class=\"navPages-action navPages-action-end\" href=\"/wishlist.php\" aria-label=\"{{lang 'account.wishlists.heading'}}\">\n                <span class=\"text\">\n                    <svg class=\"icon\" aria-hidden=\"true\">\n                        <use xlink:href=\"#icon-heart\"></use>\n                    </svg>\n                    {{lang 'account.wishlists.heading'}}\n                </span>\n            </a>\n        </li>\n        {{#if settings.phone_number}}\n            <li class=\"navPages-item navPages-item-user\">\n                <a class=\"navPages-action navPages-action-end\" href=\"tel:{{settings.phone_number}}\" aria-label=\"Phone number {{settings.phone_number}}\">\n                    <span class=\"text\">\n                        <svg class=\"icon\" aria-hidden=\"true\">\n                            <use xlink:href=\"#icon-phone\"></use>\n                        </svg>\n                        {{settings.phone_number}}\n                    </span>\n                </a>\n            </li>\n        {{/if}}\n        {{#if currency_selector.currencies.length '>' 1}}\n            <li class=\"navPages-item navPages-item-user has-dropdown\">\n                <p class=\"navPages-action navPages-action-depth-max has-subMenu is-root\">\n                    <a class=\"text\" href=\"#\">\n                        <span class=\"flag\">{{{currency_selector.active_currency_flag}}}</span> \n                        {{currency_selector.active_currency_code}}\n                    </a>\n                    <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                        <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                    </span>\n                </p>\n                <div class=\"navPage-subMenu navPage-subMenu-horizontal\">\n                    <ul class=\"navPage-subMenu-list\">\n                        <li class=\"navPage-subMenu-item-child navPage-subMenu-title\">\n                            <p class=\"navPage-subMenu-action navPages-action\">\n                                <span class=\"navPages-action-moreIcon\" aria-hidden=\"true\">\n                                    <svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-chevron-down\" /></svg>\n                                    <span>back</span>\n                                </span>\n                                <a class=\"text\" href=\"#\">\n                                    {{currency_selector.active_currency_code}}\n                                </a>\n                            </p>\n                        </li>\n                        {{#each currency_selector.currencies}}\n                            <li class=\"navPage-subMenu-item-child navPages-action-end\">\n                                <a class=\"navPage-subMenu-action navPages-action\" href=\"{{{switch_url}}}\">\n                                    <span class=\"text\"><span class=\"flag\">{{{flag}}}</span>{{name}}</span>\n                                </a>\n                            </li>\n                        {{/each}}\n                    </ul>\n                </div>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links}}\n            <li class=\"navPages-item navPages-item-user\">\n                <div class=\"navPages-action navPages-action-end\">\n                    <div class=\"text\">\n                        {{> components/common/social-links position=\"newsletter\"}}\n                    </div>\n                </div>\n            </li>\n        {{/if}}\n    </ul>\n</nav>\n",
  "components/halothemes/halo-account-popup": "<div id=\"{{data}}-popup\" class=\"account-popup {{#if data '==' 'account-pc'}}halo-auth-popup{{/if}}\">\n    <div class=\"halo-popup-content\">\n        <ul class=\"account-dropdown\">\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.orders.all}}\">\n                    <span class=\"text\">{{lang 'account.nav.orders'}}</span>\n                </a>\n            </li>\n            {{#if settings.returns_enabled}}\n                <li class=\"account-item\">\n                    <a class=\"account-action\" href=\"{{urls.account.returns}}\">\n                        <span class=\"text\">{{lang 'account.nav.returns'}}</span>\n                    </a>\n                </li>\n            {{/if}}\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.inbox}}\">\n                    <span class=\"text\">Messages</span>\n                </a>\n            </li>\n             <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.addresses}}\">\n                    <span class=\"text\">{{lang 'account.nav.addresses'}}</span>\n                </a>\n            </li>\n            {{#if settings.show_payment_methods}}\n                <li class=\"account-item\">\n                    <a class=\"account-action\" href=\"{{urls.account.payment_methods.all}}\">\n                        <span class=\"text\">{{lang 'account.nav.payment_methods'}}</span>\n                    </a>\n                </li>\n            {{/if}}\n            {{#if settings.show_wishlist}}\n                <li class=\"account-item\">\n                    <a class=\"account-action\" href=\"{{urls.account.wishlists.all}}\">\n                        <span class=\"text\">Wishlist</span>\n                    </a>\n                </li>\n            {{/if}}\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.recent_items}}\">\n                    <span class=\"text\">{{lang 'account.nav.recently_viewed'}}</span>\n                </a>\n            </li>\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.account.details}}\">\n                    <span class=\"text\">{{lang 'account.nav.settings'}}</span>\n                </a>\n            </li>\n            <li class=\"account-item\">\n                <a class=\"account-action\" href=\"{{urls.auth.logout}}\">\n                    <span class=\"text\">{{lang 'common.logout'}}</span>\n                </a>\n            </li>\n        </ul>\n    </div>\n</div>\n",
  "components/halothemes/halo-before-you-leave": "<div class=\"before-you-leave-banner\">\n\t{{#if theme_settings.halo-before-you-leave-banner-img}}\n\t\t<div class=\"image\">\n\t\t\t<img class=\"lazyload\" src=\"{{cdn 'img/loading.svg'}}\" data-src=\"https://cdn11.bigcommerce.com/s-{{settings.store_hash}}/product_images/uploaded_images/{{theme_settings.halo-before-you-leave-banner-img}}\" alt=\"{{theme_settings.halo-before-you-leave-title}}\" title=\"{{theme_settings.halo-before-you-leave-title}}\">\n\t\t</div>\n\t{{/if}}\n\t<div class=\"content{{#unless theme_settings.halo-before-you-leave-banner-img}} noImage{{/unless}}\">\n\t\t{{#if theme_settings.halo-before-you-leave-logo}}\n\t\t\t<a class=\"store-logo\" href=\"/\">\n\t\t\t\t{{#if settings.store_logo.image}}\n\t            \t<img src=\"{{getImage settings.store_logo.image 'logo_size'}}\" alt=\"{{settings.store_logo.title}}\" title=\"{{settings.store_logo.title}}\">\n\t            {{else}}\n\t            \t<span>{{settings.store_logo.title}}</span>\n\t            {{/if}}\n\t        </a>\n\t\t{{/if}}\n\t\t{{#if theme_settings.halo-before-you-leave-title}}\n\t\t\t<h3 class=\"title\">{{theme_settings.halo-before-you-leave-title}}</h3>\n\t\t{{/if}}\n\t\t{{#if theme_settings.halo-before-you-leave-text-1}}\n\t\t\t<p class=\"desc1\">{{theme_settings.halo-before-you-leave-text-1}}</p>\n\t\t{{/if}}\n\t\t{{#if theme_settings.halo-before-you-leave-subheading-1}}\n\t\t\t<h3 class=\"heading1\">{{theme_settings.halo-before-you-leave-subheading-1}}</h3>\n\t\t{{/if}}\n\t\t{{#if theme_settings.halo-before-you-leave-text-2}}\n\t\t\t<p class=\"desc2\">{{theme_settings.halo-before-you-leave-text-2}}</p>\n\t\t{{/if}}\n\t\t{{#if theme_settings.halo-before-you-leave-subheading-2}}\n\t\t\t<h3 class=\"heading2\">{{theme_settings.halo-before-you-leave-subheading-2}}</h3>\n\t\t{{/if}}\n\t\t{{#if theme_settings.halo-before-you-leave-banner-btn-text}}\n\t\t\t<a class=\"button button--primary\" href=\"#\">\n\t\t\t\t<span class=\"text\">{{theme_settings.halo-before-you-leave-banner-btn-text}}</span>\n\t\t\t</a>\n\t\t{{/if}}\n\t</div>\n</div>\n<div class=\"before-you-leave-tab\">\n\t<div class=\"tabs-wrapper\">\n\t\t<ul class=\"tabs\" data-tab>\n            {{#if theme_settings.halo-before-you-leave-recommended-text}}\n                <li class=\"tab is-active recommended\" data-id=\"recommended\">\n                    <a class=\"tab-title\" href=\"#tab-recommended\">{{theme_settings.halo-before-you-leave-recommended-text}}<span class=\"count\">0</span></a>\n                </li>\n            {{/if}}\n        </ul>\n        {{#if theme_settings.halo-before-you-leave-search}}\n\t        <div class=\"before-you-leave-search\">\n\t        \t<a class=\"search-icon\" href=\"#\" data-search-popup aria-label=\"{{lang 'search.quick_search.input_label'}}\">\n\t                <svg class=\"icon icon-search\" aria-label=\"{{lang 'ada.open_searh'}}\"><use xlink:href=\"#icon-search2\"></use></svg>\n\t                <svg class=\"icon icon-close\" aria-label=\"{{lang 'ada.close_search'}}\"><use xlink:href=\"#icon-close\"></use></svg>\n\t            </a>\n\t            {{> components/common/quick-search-2}}\n\t        </div>\n        {{/if}}\n\t</div>\n    <div class=\"tabs-contents\">\n        {{#if theme_settings.halo-before-you-leave-recommended-text}}\n            <div class=\"tab-content is-active\" id=\"tab-recommended\">\n            \t<div id=\"recommended-products-list-tmp\">\n            \t\t<div class=\"product-slider\"></div>\n            \t</div>\n          </div>\n        {{/if}}\n    </div>\n</div>\n{{#if theme_settings.halo-before-you-leave-back-text}}\n\t<div class=\"before-you-leave-back\">\n\t\t<a class=\"button\" href=\"/\">\n\t\t\t<span class=\"text\">{{theme_settings.halo-before-you-leave-back-text}}</span>\n\t\t</a>\n\t</div>\n{{/if}}\n",
  "components/halothemes/halo-login-popup": "<div id=\"{{data}}-popup\" class=\"login-form-popup {{#if data '==' 'login-pc'}}halo-auth-popup{{/if}}\">\n    <div class=\"halo-popup-content\">\n        <form class=\"form\" action=\"{{settings.secure_base_url}}/login.php?action=check_login\" method=\"post\">\n            <div class=\"login-form-wrapper\">\n                <div class=\"form-field\">\n                    <label class=\"form-label\" for=\"{{email_id}}\">\n                        {{lang 'login.field_email'}}\n                        <em class=\"text-danger\">*</em>\n                    </label>\n                    <input id=\"{{email_id}}\" class=\"form-input\" type=\"email\" value=\"\" name=\"login_email\" placeholder=\"{{lang 'login.field_email2'}}\">\n                </div>\n                <div class=\"form-field\">\n                    <label class=\"form-label\" for=\"{{password_id}}\">\n                        {{lang 'login.field_password'}}\n                        <em class=\"text-danger\">*</em>\n                    </label>\n                    <input id=\"{{password_id}}\" class=\"form-input\" type=\"password\" value=\"\" placeholder=\"{{lang 'login.field_password'}}\" name=\"login_pass\">\n                </div>\n                <div class=\"form-actions text-center\">\n                    <button type=\"button\" class=\"button button--primary\">{{lang 'login.submit_value' }}</button>\n                    <a class=\"forgot-password\" href=\"{{urls.auth.forgot_password}}\">{{lang 'login.forgot_password'}}</a>\n                    <a class=\"create-account button\" href=\"{{urls.auth.create_account}}\">{{lang 'login.new_customer.create_account'}}</a>\n                </div>\n            </div>\n        </form>\n    </div>\n</div>\n",
  "components/common/social-links": "<ul class=\"socialLinks socialLinks--alt\">\n\t{{#if position '==' 'footer'}}\n        {{#each social_media}}\n            <li class=\"socialLinks-item socialLinks-item--{{display_name}}\">\n                <a class=\"icon icon--{{display_name}}\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-messenger\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n\t{{#if position '==' 'newsletter'}}\n\t\t{{#each social_media}}\n            <li class=\"socialLinks-item\">\n                <a class=\"icon icon--{{display_name}}2\" href=\"{{url}}\" target=\"_blank\" rel=\"noopener\" title=\"Open {{{capitalize display_name}}} in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-{{display_name}}2\" /></svg>\n                </a>\n            </li>\n        {{/each}}\n        {{#if theme_settings.show_social-links-messenger}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--messenger2\" href=\"{{theme_settings.show_social-links-messenger}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Messenger in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-messenger2\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n        {{#if theme_settings.show_social-links-snapchat}}\n        \t<li class=\"socialLinks-item\">\n                <a class=\"icon icon--snapchat2\" href=\"{{theme_settings.show_social-links-snapchat}}\" target=\"_blank\" rel=\"noopener\" title=\"Open Snapchat in a new tab\">\n                    <svg aria-hidden=\"true\"><use xlink:href=\"#icon-snapchat2\" /></svg>\n                </a>\n            </li>\n        {{/if}}\n\t{{/if}}\n</ul>\n\n",
  "components/common/quick-search-2": "<form class=\"form\" action=\"{{urls.search}}\">\n    <fieldset class=\"form-fieldset\">\n        <div class=\"form-field\">\n            <label class=\"is-srOnly\" for=\"search_query2\">{{lang \"search.quick_search.input_label\"}}</label>\n            <input class=\"form-input\" data-search-quick name=\"search_query2\" id=\"search_query2\" data-error-message=\"{{lang 'search.error.empty_field'}}\" placeholder=\"{{lang 'search.quick_search.input_placeholder'}}\" autocomplete=\"off\">\n            <button type=\"submit\" aria-label=\"{{lang 'search.quick_search.button_search'}}\"><svg class=\"icon\" aria-hidden=\"true\"><use xlink:href=\"#icon-search2\"></use></svg></button>\n        </div>\n    </fieldset>\n</form>\n"
