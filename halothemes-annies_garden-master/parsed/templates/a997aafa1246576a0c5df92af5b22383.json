{
  "components/products/options/set-select": "<div class=\"form-field\" data-product-attribute=\"set-select\" data-product-option-id=\"{{id}}\">\n    <label class=\"form-label form-label--alternate form-label--inlineSmall\" for=\"attribute_select_{{id}}\">\n        {{ this.display_name }}\n        {{> components/common/requireness-msg}}\n        <span data-option-value></span>\n    </label>\n\n    <select class=\"form-select\" name=\"attribute[{{this.id}}]\" id=\"attribute_select_{{id}}\" {{#if required}}required{{/if}}>\n        <option value=\"\">{{lang 'products.choose_options'}}</option>\n        {{#each this.values}}\n            <option data-product-attribute-value=\"{{id}}\" value=\"{{id}}\" {{#if selected}}selected data-default{{/if}} data-title=\"{{label}}\" data-index=\"{{@index}}\">{{label}}</option>\n        {{/each}}\n    </select>\n</div>\n",
  "components/common/requireness-msg": "<small>\n    {{#if required}}{{lang 'common.required'}}{{else}}{{lang 'common.optional'}}{{/if}}\n</small>\n"
}